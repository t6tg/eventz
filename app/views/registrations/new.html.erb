<h1>Registration for <%= link_to @event.name, event_path(@event) %></h1>
<%= form_with model: [@event, @registrations] do |f| %>
  <%= render 'shared/errors' , object: @registrations%>
  <%= f.label :how_heard %>
  <%= f.select :how_heard, Registration::HOW_HEARD_OPTIONS, prompt: "Pick one" %>
  <%= f.submit %>
<% end %>
